<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:visibility="visible"
    tools:context=".Layouts.StudentLanding">


    <View
        android:id="@+id/topRed"
        android:layout_width="435dp"
        android:layout_height="164dp"
        android:background="#990000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/montserrat_semibold"
        android:text="trojan"
        android:textColor="@color/white"
        android:textSize="25sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/montserrat"
        android:text="check in/out"
        android:textColor="@color/white"
        android:textSize="25sp"
        app:layout_constraintStart_toEndOf="@+id/textView14"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/welcomeMessage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/montserrat"
        android:text="@string/welcome_mname_is_jeff"
        android:textColor="@color/white"
        android:textSize="15sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView14" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/fab"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:clickable="true"
        app:backgroundTint="#00FFCC00"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:maxImageSize="62dp"
        app:srcCompat="@drawable/ic_outline_account_circle_24"
        app:tint="@null" />

    <TextView
        android:id="@+id/Actions"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:fontFamily="@font/montserrat_semibold"
        android:text="Actions"
        android:textColor="#707070"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.081"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/infoView" />

    <Button
        android:id="@+id/signOut"
        android:layout_width="389dp"
        android:layout_height="38dp"
        android:layout_marginBottom="16dp"
        android:fontFamily="@font/montserrat_semibold"
        android:stateListAnimator="@null"
        android:text="@string/sign_out"
        android:textSize="12sp"
        app:backgroundTint="#990000"
        app:cornerRadius="50dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.511"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/divider" />

    <View
        android:id="@+id/divider"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_marginBottom="80dp"
        android:background="#FFCC00"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/checkOutShadow"
        android:layout_width="176dp"
        android:layout_height="176dp"
        android:layout_marginTop="68dp"
        android:layout_marginEnd="28dp"
        android:background="@drawable/round_rectangle"
        android:clickable="false"
        android:fontFamily="@font/montserrat_semibold"
        android:stateListAnimator="@null"
        android:textSize="12sp"
        app:backgroundTint="#d8d8d8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/infoView" />

    <Button
        android:id="@+id/checkOut"
        android:layout_width="175dp"
        android:layout_height="175dp"
        android:background="@drawable/round_rectangle"
        android:fontFamily="@font/montserrat_semibold"
        android:stateListAnimator="@null"
        android:textSize="12sp"
        app:backgroundTint="#ffffff"
        app:layout_constraintBottom_toBottomOf="@+id/checkOutShadow"
        app:layout_constraintEnd_toEndOf="@+id/checkOutShadow"
        app:layout_constraintStart_toStartOf="@+id/checkOutShadow"
        app:layout_constraintTop_toTopOf="@+id/checkOutShadow" />

    <ImageView
        android:id="@+id/checkOutIcon"
        android:layout_width="125dp"
        android:layout_height="129dp"
        android:background="@drawable/round_qr"
        android:scaleX=".6"
        android:scaleY=".6"
        app:layout_constraintBottom_toTopOf="@+id/checkoutTitle"
        app:layout_constraintEnd_toEndOf="@+id/checkOutShadow"
        app:layout_constraintStart_toStartOf="@+id/checkOutShadow"
        app:layout_constraintTop_toTopOf="@+id/checkOutShadow"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/ic_outline_exit_to_app_24" />

    <Button
        android:id="@+id/ScanShadow"
        android:layout_width="176dp"
        android:layout_height="176dp"
        android:layout_marginTop="68dp"
        android:background="@drawable/round_rectangle"
        android:clickable="false"
        android:fontFamily="@font/montserrat_semibold"
        android:stateListAnimator="@null"
        android:textSize="12sp"
        app:backgroundTint="#d8d8d8"
        app:layout_constraintEnd_toStartOf="@+id/checkOutShadow"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/infoView" />

    <Button
        android:id="@+id/Scan"
        android:layout_width="175dp"
        android:layout_height="175dp"
        android:background="@drawable/round_rectangle"
        android:fontFamily="@font/montserrat_semibold"
        android:stateListAnimator="@null"
        android:textSize="12sp"
        app:backgroundTint="#ffffff"
        app:layout_constraintBottom_toBottomOf="@+id/ScanShadow"
        app:layout_constraintEnd_toEndOf="@+id/ScanShadow"
        app:layout_constraintStart_toStartOf="@+id/ScanShadow"
        app:layout_constraintTop_toTopOf="@+id/ScanShadow" />

    <ImageView
        android:id="@+id/checkoutImage"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:background="@drawable/round_qr"
        android:scaleX=".6"
        android:scaleY=".6"
        app:layout_constraintBottom_toTopOf="@+id/scanTitle"
        app:layout_constraintEnd_toEndOf="@+id/ScanShadow"
        app:layout_constraintHorizontal_bias="0.512"
        app:layout_constraintStart_toStartOf="@+id/ScanShadow"
        app:layout_constraintTop_toTopOf="@+id/Scan"
        app:srcCompat="@drawable/ic_baseline_qr_code_24" />

    <View
        android:id="@+id/infoView"
        android:layout_width="382dp"
        android:layout_height="230dp"
        android:layout_marginTop="104dp"
        android:background="@drawable/round_rectangle"
        app:cornerRadius="500dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.58"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/id2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:fontFamily="@font/montserrat"
        android:text="Kickin Ass and Taking Names"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="@+id/infoView"
        app:layout_constraintHorizontal_bias="0.801"
        app:layout_constraintStart_toEndOf="@+id/majorTitle"
        app:layout_constraintTop_toBottomOf="@+id/viewDivider2" />

    <View
        android:id="@+id/viewDivider2"
        android:layout_width="350dp"
        android:layout_height="1dp"
        android:background="#707070"
        app:layout_constraintBottom_toTopOf="@+id/viewDivider"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/topRed" />

    <View
        android:id="@+id/viewDivider1"
        android:layout_width="350dp"
        android:layout_height="1dp"
        android:background="#707070"
        app:layout_constraintBottom_toTopOf="@+id/viewDivider2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/infoView" />

    <View
        android:id="@+id/viewDivider"
        android:layout_width="350dp"
        android:layout_height="1dp"
        android:layout_marginBottom="60dp"
        android:background="#707070"
        app:layout_constraintBottom_toBottomOf="@+id/infoView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.548"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/nameTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:fontFamily="@font/montserrat"
        android:text="Name"
        android:textColor="#000000"
        app:layout_constraintBottom_toTopOf="@+id/viewDivider1"
        app:layout_constraintStart_toStartOf="@+id/infoView"
        app:layout_constraintTop_toTopOf="@+id/infoView"
        app:layout_constraintVertical_bias="0.533" />

    <TextView
        android:id="@+id/majorTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="12dp"
        android:fontFamily="@font/montserrat"
        android:text="Major"
        android:textColor="#000000"
        app:layout_constraintStart_toStartOf="@+id/infoView"
        app:layout_constraintTop_toBottomOf="@+id/viewDivider2" />

    <TextView
        android:id="@+id/buildingName"
        android:layout_width="291dp"
        android:layout_height="37dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/montserrat"
        android:text="N/A"
        android:textColor="#000000"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="@+id/infoView"
        app:layout_constraintHorizontal_bias="0.747"
        app:layout_constraintStart_toStartOf="@+id/infoView"
        app:layout_constraintTop_toBottomOf="@+id/viewDivider" />

    <TextView
        android:id="@+id/scanTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="24dp"
        android:fontFamily="@font/montserrat"
        android:text="Scan"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/ScanShadow"
        app:layout_constraintEnd_toEndOf="@+id/ScanShadow"
        app:layout_constraintHorizontal_bias="0.516"
        app:layout_constraintStart_toStartOf="@+id/Scan" />

    <TextView
        android:id="@+id/checkoutTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="20dp"
        android:fontFamily="@font/montserrat"
        android:text="Check Out"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/checkOut"
        app:layout_constraintEnd_toEndOf="@+id/checkOut"
        app:layout_constraintHorizontal_bias="0.51"
        app:layout_constraintStart_toStartOf="@+id/checkOut" />

    <TextView
        android:id="@+id/idTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/montserrat"
        android:text="ID"
        android:textColor="#000000"
        app:layout_constraintBottom_toTopOf="@+id/viewDivider2"
        app:layout_constraintStart_toStartOf="@+id/infoView"
        app:layout_constraintTop_toBottomOf="@+id/viewDivider1"
        app:layout_constraintVertical_bias="0.055" />

    <TextView
        android:id="@+id/name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="204dp"
        android:fontFamily="@font/montserrat"
        android:text="Mname Is Jeff"
        android:textColor="#000000"
        app:layout_constraintBottom_toTopOf="@+id/viewDivider1"
        app:layout_constraintStart_toEndOf="@+id/nameTitle"
        app:layout_constraintTop_toTopOf="@+id/infoView"
        app:layout_constraintVertical_bias="0.533" />

    <TextView
        android:id="@+id/id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/montserrat"
        android:text="1234567890"
        android:textColor="#000000"
        app:layout_constraintBottom_toTopOf="@+id/viewDivider2"
        app:layout_constraintEnd_toEndOf="@+id/infoView"
        app:layout_constraintHorizontal_bias="0.905"
        app:layout_constraintStart_toEndOf="@+id/idTitle"
        app:layout_constraintTop_toBottomOf="@+id/viewDivider1" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="48dp"
        android:layout_height="49dp"
        app:layout_constraintBottom_toBottomOf="@+id/infoView"
        app:layout_constraintEnd_toStartOf="@+id/buildingName"
        app:layout_constraintStart_toStartOf="@+id/infoView"
        app:layout_constraintTop_toBottomOf="@+id/viewDivider"
        app:srcCompat="@drawable/ic_baseline_location_on_24" />


</androidx.constraintlayout.widget.ConstraintLayout>